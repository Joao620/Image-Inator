{
  "name": "mosaic-inator",
  "version": "1.0.0",
  "description": "A image mosaic tool with GPU acceleration for javascript that works in node and the browser",
  "main": "./dist/index.js",
  "type": "commonjs",
  "types": "./dist",
  "browser": {},
  "browserify-shim": {
    "gpu.js": "global:GPU"
  },
  "scripts": {
    "start": "ts-node src/debugger.ts",
    "dev": "watchify ./src/browser.ts -p [ tsify ] -r ./src/teste.ts:fs | node",
    "build": "browserify -e ./src/browser.ts -p [ tsify ] -r ./src/teste.ts:fs | node"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "canvas": "^2.8.0",
    "gpu.js": "^2.11.4",
    "image-size": "^1.0.0",
    "yargs": "^17.2.1"
  },
  "devDependencies": {
    "@types/yargs": "^17.0.3",
    "@types/node": "^16.11.11",
    "browserify-shim": "^3.8.14",
    "source-map-support": "^0.5.20",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.2",
    "watchify": "^4.0.0"
  }
}
